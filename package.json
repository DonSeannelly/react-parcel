{
  "name": "react-parcel",
  "version": "0.0.0",
  "description": "A simple react app using the parcel bundler.",
  "main": "index.js",
  "scripts": {
    "build": "node_modules/.bin/parcel build --no-cache src/index.html",
    "start": "node_modules/.bin/parcel src/index.html --open",
    "test": "node_modules/.bin/jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DonSeannelly/react-parcel.git"
  },
  "author": "Sean Donnelly",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DonSeannelly/react-parcel/issues"
  },
  "homepage": "https://github.com/DonSeannelly/react-parcel#readme",
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/react": "^16.0.41",
    "@types/react-dom": "^16.0.4",
    "jest": "^22.4.3",
    "node-sass": "^4.8.3",
    "parcel-bundler": "^1.6.2",
    "ts-jest": "^22.4.2",
    "typescript": "^2.7.2"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": ".*?(?=.spec).*?.(js|ts|jsx|tsx)",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  }
}
